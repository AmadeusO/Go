<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="270.0" prefWidth="407.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tp.client.MainMenuController">
   <children>
      <Button  fx:id="startButton" mnemonicParsing="false" onAction="#switchToModeMenu" prefHeight="26.0" prefWidth="299.0" text="START" />
      <Button fx:id="loadButton" mnemonicParsing="false" onAction="#switchToLoadMenu" prefHeight="26.0" prefWidth="299.0" text="LOAD" />
      <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exit" prefHeight="26.0" prefWidth="299.0" text="EXIT" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
