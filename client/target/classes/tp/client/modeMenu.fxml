<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tp.client.ModeMenuController">
    <children>
        <Button fx:id="signleplayerButton" mnemonicParsing="false" onAction="#startSignleplayer" prefHeight="26.0" prefWidth="299.0" text="PLAY BOT" />
        <Button fx:id="multiplayerButton" mnemonicParsing="false" onAction="#startMultiplayer" prefHeight="26.0" prefWidth="299.0" text="PLAY FRIEND" />
        <Button fx:id="returnButton" mnemonicParsing="false" onAction="#returnToMainMenu" prefHeight="26.0" prefWidth="299.0" text="RETURN" />

    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
